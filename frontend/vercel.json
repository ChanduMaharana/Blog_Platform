{
  "version": 2,
  "builds": [
    {
      "src": "dist/blog-platform/server/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/blog-platform/server/main.js"
    }
  ],
  "functions": {
    "dist/blog-platform/server/server.mjs": {
      "runtime": "nodejs20.x",
      "maxDuration": 60
    }
  }
}

